int main() {
    printInt(21 % 13);
    printInt(-21 % 13);
    printInt(21 % -13);
    printInt(-21 % -13);
    printInt(21 % 7);
    printInt(-21 % 7);
    printInt(21 % -7);
    printInt(-21 % -7);
    printInt(21 / 4);
    printInt(-21 / 4);
    printInt(21 / -4);
    printInt(-21 / -4);
    printInt(21 / 3);
    printInt(-21 / 3);
    printInt(21 / -3);
    printInt(-21 / -3);
    return 0;
}
